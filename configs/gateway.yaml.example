rpc:
    user: "localhost:10001"
    video_list: "localhost:10002"
    video: "localhost:10003"
    social: "localhost:10004"

web:
    addr: ":10000"
log:
  # 环境 dev | prd
  env: dev
  # 输出日志路径
  path: ./log/gateway.log
  # 日志格式 json|console (不填默认console)
  encoding: console
  # 单个文件最大尺寸，默认单位 M  (不填默认100)
  max_size: 10
  # 最大时间，默认单位 day (不填默认不删除备份)
  max_age: 30
  # 最多保留备份个数 (不填默认不删除备份)
  max_backups: 30